    "version": "0.2.0",
    "configurations": [

       
        {
           "name": "Listen for XDebug",
            "type": "php",
            "request": "launch",
            "pathMappings": {
               "/siba": "${workspaceRoot}"
            },
                "port": 9000
            },
        {
            "name": "Launch currently open script",
            "type": "php",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "port": 9000,
            "request": "launch"
        }
    ]
}